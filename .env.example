# Test Configuration for Supreme Court RAG System
# Copy this to .env and fill in your actual values

# Database Configuration
DATABASE_URL=postgresql://user:password@localhost:5432/courtsight_db

# Google Cloud Configuration
PROJECT_ID=g-72-courtsightteam
GOOGLE_APPLICATION_CREDENTIALS=/path/to/your/service-account-key.json

# Vertex AI Configuration
VERTEX_AI_ENDPOINT_ID=your-vertex-ai-endpoint-id
VERTEX_AI_LOCATION=us-central1

# Optional: Custom settings
COLLECTION_NAME=supreme_court_docs
ENABLE_METADATA_EXTRACTION=true
ENABLE_GUARDRAILS_VALIDATION=true
ENABLE_RAGAS_EVALUATION=true

# Logging Configuration
LOG_LEVEL=INFO

# Security Configuration (for production)
SECRET_KEY=your-secret-key-here
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30
